{
  "name": "toolkeeper",
  "version": "1.0.5",
  "description": "Tool Crib Web Application",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node ./src/server.js",
    "dev": "nodemon ./src/server.js",
    "build:css": "npx tailwindcss -c ./src/config/tailwind.config.cjs --postcss ./src/config/postcss.config.cjs  -i ./src/public/css/tailwind.css -o ./src/public/css/style.css",
    "watch:css": "npx tailwindcss -c ./src/config/tailwind.config.cjs --postcss ./src/config/postcss.config.cjs  -i ./src/public/css/tailwind.css -o ./src/public/css/style.css -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaveLuhman/toolkeeper.git"
  },
  "keywords": [],
  "author": "Dave Luhman",
  "license": "",
  "bugs": {
    "url": "https://github.com/DaveLuhman/toolkeeper/issues"
  },
  "homepage": "https://github.com/DaveLuhman/toolkeeper#readme",
  "dependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "autoprefixer": "^10.4.13",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "colors": "^1.4.0",
    "connect-mongodb-session": "^3.1.1",
    "cookie-parser": "^1.4.6",
    "daisyui": "^2.50.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "express-flash": "^0.0.2",
    "express-handlebars": "^6.0.7",
    "express-rate-limit": "^6.7.0",
    "express-session": "^1.17.3",
    "handlebars-helpers": "^0.10.0",
    "handlebars-paginate": "^0.1.0",
    "helmet": "^6.0.1",
    "mongoose": "^6.9.0",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "postcss": "^8.4.21",
    "tailwindcss-dropdown": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
