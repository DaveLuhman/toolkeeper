{{#eq onboarding.progress.importSetup false}}
    <script>
        introJs().addSteps([
            {
                element: '#settings_import',
                title: 'Import Items via CSV',
                intro: 'Select the type of items you want to import from the list below.  We highly recommend doing them in the order listed, as tools are dependnent on categories and SAs and can fail to import if their dependencies are not imported first.'
            },
            {
                element: '#startImportButton',
                title: 'Start Import',
                intro: 'Click here to begin the import process after configuring your settings.'
            },
        ]);
        introJs().onexit(function () {
            $.post('/settings/import/onboarding-complete')
        });
    </script>
{{/eq}}